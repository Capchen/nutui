<nut-transition 
  name="fade" 
  show='{{ show }}' 
  custom-class="nut-overlay custom-class" 
  custom-style='z-index:{{zIndex}};{{overlayStyle}}'   
  bind:tap='onClick'
  duration='{{duration}}'> 
    <slot></slot>
</nut-transition>