.nut-cell-group {
  display: block;
}
.nut-cell-group__title {
  display: inherit;
  padding: 0 20rpx;
  color: #909ca4;
  font-size: 28rpx;
  line-height: 40rpx;
  margin-top: 30px;
  margin-bottom: 10px;
}
.nut-cell-group__desc {
  display: inherit;
  padding: 0 20rpx;
  color: #909ca4;
  font-size: 24rpx;
  line-height: 32rpx;
  margin-top: 10px;
  margin-bottom: 10px;
}
.nut-cell-group__warp {
  display: inherit;
  border-radius: 12rpx;
  overflow: hidden;
  background-color: #fff;
  margin: 10px 0;
}