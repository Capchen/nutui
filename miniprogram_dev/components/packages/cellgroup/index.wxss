.nut-cell-group {
  display: block;
}
.nut-cell-group__title {
  display: inherit;
  padding: 0 10px;
  color: #909ca4;
  font-size: 14px;
  line-height: 20px;
  margin-top: 30px;
  margin-bottom: 10px;
}
.nut-cell-group__desc {
  display: inherit;
  padding: 0 10px;
  color: #909ca4;
  font-size: 12px;
  line-height: 16px;
  margin-top: 10px;
  margin-bottom: 10px;
}
.nut-cell-group__warp {
  display: inherit;
  border-radius: 6px;
  overflow: hidden;
  background-color: #fff;
  margin: 10px 0;
}